{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "SidCodeBuildAccess",
        "Action": [
            "codebuild:*",
            "s3:GetBucketLocation",
            "s3:ListAllMyBuckets",
            "s3:GetBucketAcl",
            "s3:GetObject",
            "s3:List*",
            "s3:PutObject",
            "secretsmanager:GetSecretValue",
            "kms:Decrypt",
            "logs:FilterLogEvents",
            "logs:GetLogEvents"
        ],
        "Effect": "Allow",
        "Resource": "*"
      }
    ]
  }